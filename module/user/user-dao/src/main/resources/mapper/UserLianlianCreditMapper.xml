<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fqgj.youjie.user.dao.impl.UserLianlianCreditDaoImpl">

    <select id="selectByBankId" parameterType="Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from user_lianlian_credit
        where bank_id = #{bankId,jdbcType=BIGINT} and deleted=0 limit 1
    </select>

</mapper>