<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fqgj.youjie.user.dao.impl.UserGpsDaoImpl">
    <select id="selectGpsByUserId" resultMap="BaseResultMap">
        SELECT * FROM user_gps WHERE user_id = #{userId,jdbcType=BIGINT} AND deleted=0
        ORDER BY gmt_create DESC LIMIT 0,50
    </select>
</mapper>